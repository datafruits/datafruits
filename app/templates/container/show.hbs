{{#if this.model.tracks}}
  <Container::DatafruitsPlayer @track={{get this.model.tracks "0"}} />
{{else}}
  <Container::DatafruitsPlayer />
{{/if}}
<a
  class="logo"
  href="https://datafruits.fm/"
  target="_blank" rel="noopener noreferrer" aria-label={{t "site_title_fm"}}
>
  <img id="circle-logo" src="/assets/images/logo.png" alt={{t "site_title"}}>
</a>
