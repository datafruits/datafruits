{{#each this.currentUser.user.trackFavorites as |trackFavorite|}}
  <PodcastTrack
    @track={{trackFavorite.track}}
    @scheduledShow={{trackFavorite.track.scheduledShow}}
    @soundcloudKey={{trackFavorite.track.soundcloudKey}}
    @mixcloudKey={{trackFavorite.track.mixcloudKey}}
    @selectedLabels={{this.selectedLabels}}
  />
{{/each}}
