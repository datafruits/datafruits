<PowerSelectMultiple
  @options={{this.badges}}
  @selected={{this.selectedBadges}}
  @selectedItemComponent={{component 'djs/selected-badge'}}
  @allowClear={{true}}
  @placeholder="Select some badges..."
  @onChange={{this.changeBadges}} as |badge|
>
  <img class="w-8" src={{badge.img}}/>
</PowerSelectMultiple>